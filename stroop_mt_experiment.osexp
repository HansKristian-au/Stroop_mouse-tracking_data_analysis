define inline_script CheckStartClick
	set description "Executes Python code"
	___run__
	# --- Inline_script: check_start_click (RUN tab) ---
	# This runs immediately after start_mouse
	
	# Did the click hit our ROI?
	clicked_rois = cursor_roi.split(';') if cursor_roi else []
	
	if 'start_button' in clicked_rois:
	    # Valid START click
	    start_ok = 1
	
	    # Recenter mouse at button center (same as in the sketchpad)
	    m = Mouse()
	    m.show_cursor(True)        # make sure it is visible right when START is clicked
	    m.set_pos((0, 450))        # use your actual button centre here
	else:
	    # Click somewhere else: not acceptable
	    start_ok = 0
	__end__
	set _prepare ""

