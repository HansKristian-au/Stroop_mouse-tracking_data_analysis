define inline_script set_feedback
	set description "Executes Python code"
	___run__
	# Trial-wise feedback logic
	
	# Case 1: Too slow (no response within 2500 ms)
	if response == "None" or response_time > 2500:
	    feedback_msg = "TIME OUT"
	
	# Case 2: Wrong key within time
	elif correct == 0:
	    feedback_msg = "X"
	
	# Case 3: Correct and fast enough
	else:
	    feedback_msg = ""
	__end__
	set _prepare ""

define inline_script shuffle_trials
	set description "Executes Python code"
	set _run ""
	___prepare__
	from datamatrix import operations as ops   # import operations
	
	loop_item = items['stimuli']   # change 'stimuli' if your loop has another name
	
	# dm is the original loop table (as defined in GUI or file), assumed 128 rows
	dm = loop_item.dm
	
	# --- Choose which of the 4 sets this participant gets ---
	# subject_nr is 1-based; we convert to 0..3
	set_index = (subject_nr - 1) % 4   # 0,1,2,3
	
	# --- First 32 rows (0–31) ---
	first32 = dm[:32]                  # 32 rows total
	subset_first32 = first32[set_index::4]  # 8 rows for this set
	
	# Duplicate those 8 rows 4 times -> 32 rows
	subset_first32 = ops.stack(
	    subset_first32,
	    subset_first32,
	    subset_first32,
	    subset_first32
	)
	
	# --- Remaining rows (32–127) ---
	rest = dm[32:]                     # 96 rows total
	subset_rest = rest[set_index::4]   # 24 rows for this set
	
	# Duplicate those 24 rows 4 times -> 96 rows
	subset_rest = ops.stack(
	    subset_rest,
	    subset_rest,
	    subset_rest,
	    subset_rest
	)
	
	# --- Shuffle within each segment ---
	subset_first32 = ops.shuffle(subset_first32)
	subset_rest = ops.shuffle(subset_rest)
	
	# New loop table: first "block" shuffled, rest shuffled -> 32 + 96 = 128 rows
	loop_item.dm = ops.stack(subset_first32, subset_rest)
	__end__

