define inline_script count_down
	set description "Executes Python code"
	set _run ""
	___prepare__
	# PREPARE tab
	canvas = Canvas()
	
	# RUN tab
	for remaining in [3, 2, 1]:
	    canvas.clear()
	    canvas.text(f'Practice Trials\n\nStarting in {remaining} …')
	    canvas.show()
	    clock.sleep(1000)
	__end__

